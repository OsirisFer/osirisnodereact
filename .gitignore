# my-fullstack-app/.gitignore

# Ignore node_modules in both server and client
server/node_modules
client/node_modules
server/.gitignore
client/.gitignore

# Logs
server/logs
*.log
server/npm-debug.log*
client/npm-debug.log*
server/yarn-debug.log*
client/yarn-debug.log*
server/yarn-error.log*
client/yarn-error.log*

# Environment variables
server/.env
client/.env.local
client/.env.development.local
client/.env.test.local
client/.env.production.local

# Production builds
server/build
server/dist
client/build

# Misc
server/.DS_Store
client/.DS_Store
server/Thumbs.db
client/Thumbs.db

# Editor directories and files
server/.idea
client/.idea
server/.vscode
client/.vscode
server/*.suo
client/*.suo
server/*.ntvs*
client/*.ntvs*
server/*.njsproj
client/*.njsproj
server/*.sln
client/*.sln

# TypeScript
server/*.tsbuildinfo
client/*.tsbuildinfo

# Service worker
client/workbox-config.js

# Testing
client/coverage
